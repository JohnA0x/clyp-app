{
  "expo": {
    "name": "clyppay",
    "slug": "clyppay",
    "owner": "gabrielclyp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ico.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splashscreen/splashscreen.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gabrielclyp.clyppay"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ico.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ]
  }
}
